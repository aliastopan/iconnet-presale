@rendermode InteractiveServer

<div class="tab-nav">
    @if (TabNavigationManager.TabNavigations.Count > 0)
    {
        @foreach (var tabNavigation in TabNavigationManager.TabNavigations)
        {
            <TabNav Tab="tabNavigation"
                OnTabChange="ChangeTab"
                OnTabClose="CloseTab"/>
        }
    }
</div>
